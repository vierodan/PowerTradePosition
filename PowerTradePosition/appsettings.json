{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Warning"
    }
  },

  "CustomLogging": {
    "Console": {
      "Enabled": true,
      "LogLevel": 0
    },
    "Seq": {
      "Enabled": true,
      "ServerUrl": "http://localhost:5341",
      "LogLevel": 0
    },
    "File": {
      "Enabled": false,
      "PathFormat": "logs/application.log",
      "LogLevel": 2
    }
  },
  
  "PowerTradeConfiguration": {
    "IntervalMinutes": 15,
    "OutputFolderPath": "~/avr/PowerTradeReports",
    "TimeZone": "Europe/Madrid"
  }
}